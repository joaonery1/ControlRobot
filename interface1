    private void text_cliente_cpfActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        
        
        btn_cliente_cadastro.setEnabled(true);
        btn_cliente_cancelar.setEnabled(true);
        text_cliente_cpf.setEnabled(true);
        text_cliente_nome.setEnabled(true);
        text_cliente_email.setEnabled(true);
       
        
       
    }                                                

    private void btn_cliente_cancelarActionPerformed(java.awt.event.ActionEvent evt) {                                                     
       btn_cliente_cadastro.setEnabled(false);
       btn_cliente_cancelar.setEnabled(false);
    }                                                    

    private void btn_cliente_cadastroActionPerformed(java.awt.event.ActionEvent evt) {                                                     
        Cliente C = new Cliente(text_cliente_cpf.getText(),text_cliente_email.getText(),text_cliente_nome.getText());
        ListaCli.add(C);
        LoadTableCli();
    }                                                    

    private void btn_cliente_novoActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        text_cliente_cpf.setText("");
        text_cliente_email.setText("");
        text_cliente_nome.setText("");
        btn_cliente_cadastro.setEnabled(true);
        btn_cliente_cancelar.setEnabled(true);
        text_cliente_nome.setEnabled(true);
        text_cliente_email.setEnabled(true);
        text_cliente_cpf.setEnabled(true);
    }                                                

